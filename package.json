{
  "name": "@flyimg/flyimg.js",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.1.0",
  "description": "Interact with a Flyimg endpoint: build URLs with the needed transformations options.",
  "license": "MIT",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "dev": "tsc -w -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "npm run typecheck",
    "verify": "npm run lint && npm test && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "build:browser": "esbuild src/client.ts --bundle --format=esm --outfile=example/vendor/flyimg-client.js",
    "example:serve": "npx --yes http-server example -c-1 -p 5173"
  },
  "keywords": [
    "flyimg",
    "image",
    "url",
    "builder",
    "yaml"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.7.5",
    "esbuild": "^0.23.1",
    "vitest": "^2.1.1",
    "typescript": "^5.6.3",
    "semantic-release": "^24.0.0",
    "@semantic-release/npm": "^12.0.0",
    "@semantic-release/github": "^10.0.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/commit-analyzer": "^12.0.0",
    "@semantic-release/release-notes-generator": "^13.0.0",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "@semantic-release/exec": "^6.0.3"
  }
}
